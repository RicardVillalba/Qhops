
<div class="dashboard-table">

<div class="dashboard-title">
<h2>Waiting List</h2>
</div>


<table>
    <thead>
        <tr>
            <th>code</th>
            <th>Name</th>
            <th>Lastname</th>
            <th>email</th>
            <th>tags</th>
            <th></th>
        </tr>
    </thead>
    {{#each queue}}
    {{#each appointments}}


    <tr>
        <td>{{this.code}}</td>
        <td>{{this.fName}}</td>
        <td>{{this.lName}}</td>
        <td>{{this.email}}</td>
        {{#each this.tags}}
        <td>{{this}}</td>
        {{/each}}


        <td>
            <div class="dashboard-buttons">
            <a href="/dashboard/to_room/{{this._id}}">
                <button class="btn">TO ROOM</button>
            </a>
            <a href="/dashboard/delete/{{this._id}}">
                <button class="btn">DELETE</button>
            </a>
            </div>
        </td>
    </tr>
    {{/each}}
    {{/each}}

</table>


<a href="/add-appointment">
    <button>add</button>
</a>




<table>
    <tr>
        <th>code</th>
        <th>Name</th>
        <th>Lastname</th>
        <th>email</th>
        <th>tags</th>
        <th>Buttons</th>
    </tr>
    {{#each queue}}
    {{#each appointments}}
    <br>

    <tr>
        <td>{{this.code}}</td>
        <td>{{this.fName}}</td>
        <td>{{this.lName}}</td>
        <td>{{this.email}}</td>
        {{#each this.tags}}
        <td>{{this}}</td>
        {{/each}}


        <td>
            <a href="/dashboard/to_room/{{this._id}}">
                <button>TO ROOM</button>
            </a>
            <a href="/dashboard/delete/{{this._id}}">
                <button>DELETE</button>
            </a>
        </td>
    </tr>
    {{/each}}
    {{/each}}

    <br>

</table>


<a href="/add-appointment">
    <button>add</button>
</a>
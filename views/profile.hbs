<div class="container">
    <div class="row justify-content-center">
        <div class="col-4 dashboard-table">
            <div class="text-center">
                <img src="{{admin.picture.image_url}}" class="profile-img " alt="profile_picture">
            </div>
            <div class="text-center">
                <form action="/profile-picture" method="POST" enctype="multipart/form-data">
                    <input type="file" name="photo" id="img" style="display:none;" />
                    <label for="img" class="btn btn-dark p-1 my-2">upload image</label>
                    <button type="submit" class="btn btn-dark p-1 my-2">Update photo</button>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="container dashboard-table table-responsive">
    <table class="table table-hover my-3">
        <thead>
            <tr>
                <th class="text-center" scope="col">code</th>
            </tr>
        </thead>
        <tbody>
            {{#each queue}}
            {{#each inProgress}}
            <tr>
                <td class="text-center">{{this.code}}</td>
            </tr>
            {{/each}}
            {{/each}}
        </tbody>
    </table>

   
    <table class="table table-hover my-3">

        <thead>
            <tr>
                <th class="text-center" scope="col">Code</th>
            </tr>
        </thead>
        <tbody>
            {{#each queue}}
            {{#each appointments}}
            <tr>
                <td class="text-center">{{this.code}}</td>
            </tr>
            {{/each}}
            {{/each}}
        </tbody>
    </table>
</div>
</div>
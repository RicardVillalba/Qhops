<div class="container">
    <div class="row">
        <div class="col-sm col-lg dashboard-table">
            <img src="{{admin.picture.image_url}}" class="profile-img" alt="profile_picture">
            <form action="/profile-picture" method="POST" enctype="multipart/form-data">
                <input type="file" name="photo" />
                <button type="submit" >Update photo</button>
            </form>

            <table>
                <thead>
                    <tr>
                        <th scope="col">code</th>
                        <th scope="col">Name</th>
                        <th scope="col">Lastname</th>
                        <th scope="col">email</th>
                        <th scope="col">tags</th>
                        <th scope="col"></th>
                    </tr>
                </thead>
                {{#each queue}}
                {{#each appointments}}


                <tr>
                    <td>{{this.code}}</td>
                    <td>{{this.fName}}</td>
                    <td>{{this.lName}}</td>
                    <td>{{this.email}}</td>
                    {{#each this.tags}}
                    <td>{{this}}</td>
                    {{/each}}


                    <td>
                        <div class="dashboard-buttons">
                            <a href="/dashboard/to_room/{{this._id}}">
                                <button class="btn">TO ROOM</button>
                            </a>
                            <a href="/dashboard/delete/{{this._id}}/{{this.status}}">
                                <button class="btn">DELETE</button>
                            </a>
                        </div>
                    </td>
                </tr>
                {{/each}}
                {{/each}}

            </table>


            <a href="/add-appointment">
                <button>add</button>
            </a>

        </div>
    </div>
</div>
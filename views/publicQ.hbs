<br>
<h1>PUBLIC QUEUE</h1>
<br>
<h2>Waiting List</h2>
<br>

<table>
    <thead>
        <tr>
            <th>code</th>
        </tr>
    </thead>
    {{#each queue}}
    {{#each appointments}}


    <tr>
        <td>{{this.code}}</td>
    </tr>
    {{/each}}
    {{/each}}

</table>

<br>

<br>
<h2>JUST CALLED IN (TOP 5 - In Progress List)</h2>
<table>
    <thead>

        <tr>
            <th>code</th>
        </tr>
    </thead>
    {{#each queue}}
    {{#each inProgress}}


    <tr>
        <td>{{this.code}}</td>
    </tr>
    {{/each}}
    {{/each}}

    <br>


</table>

<br>
<h2>Done List</h2>
<table>
    <thead>

        <tr>
            <th>code</th>
            <th>Name</th>
            <th>Lastname</th>
            <th>email</th>
            <th>tags</th>
            <th>Buttons</th>
        </tr>
    </thead>
    {{#each queue}}
    {{#each appointments_done}}


    <tr>
        <td>{{this.code}}</td>
        <td>{{this.fName}}</td>
        <td>{{this.lName}}</td>
        <td>{{this.email}}</td>
        {{#each this.tags}}
        <td>{{this}}</td>
        {{/each}}


        <td>
            <a href="/dashboard/to_room/{{this._id}}">
                <button>TO ROOM</button>
            </a>
            <a href="/dashboard/delete/{{this._id}}">
                <button>DELETE</button>
            </a>
        </td>
    </tr>
    {{/each}}
    {{/each}}

    <br>

</table>